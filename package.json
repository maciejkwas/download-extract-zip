{
  "name": "download-extract-zip",
  "version": "1.0.0",
  "description": "Script for downloading and unzipping files",
  "main": "download-and-unzip.js",
  "scripts": {
    "bundle": "pkg download-and-unzip.js",
    "test": "node download-and-unzip.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "decompress": "^4.2.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pkg": {
    "scripts": "download-and-unzip.js",
    "targets": [
      "node18-linux-x64",
      "node18-win-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist",
    "outputName": "dezipp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maciejkwas/download-extract-zip.git"
  }
}
